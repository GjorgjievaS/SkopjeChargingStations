@model IEnumerable<Demo.Models.Comment>

@{
    ViewData["Title"] = "Comments";
}

<h1>Comments/Komentari</h1>

<p>
    <a asp-action="Create" class="btn btn-primary">Create New Comment/Kreiraj nov komentar</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.komentar)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.komentar)
            </td>
            <td>
               
            </td>
        </tr>
}
    </tbody>
</table>
